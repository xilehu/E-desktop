QWidget#userAppWidget {
    background-color: transparent;
    border-radius: 20px;
    padding: 20px;
    min-width: 64px;
    min-height: 64px;
    max-width: 64px;
    max-height: 64px;
}

QWidget#indicatorWidget {
    min-width: 64px;
    min-height: 10px;
    max-width: 64px;
    max-height: 10px;
    background-color: transparent;
    border: 4px solid #ccc;
    border-radius: 20px;
    padding: 20px;
}